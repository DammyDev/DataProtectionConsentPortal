
@using Serilog;

<AuthorizeView>
    <Authorized>
        @context.User.Identity.Name!
        <a href="AzureAD/Account/SignOut" style="color:cornsilk; padding-left:3px;">Log out</a>
       
    </Authorized>
    <NotAuthorized>
        <a href="AzureAD/Account/SignIn">Log in</a>
    </NotAuthorized>
</AuthorizeView>
